#!/bin/sh
# This is the installation script for Qt, packaged in .tgz form for Slackware.
#
# Look for Qt additions to etc/profile, and if we don't find them
# then add them:
if fgrep "QTDIR/usr/lib/qt" etc/profile 1> /dev/null 2> /dev/null ; then
  # Good, we've already found the Qt stuff in etc/profile, so we
  # won't need to add it again.
  FOUND=GOOD # :^)
else
  # Add Qt environment variables to etc/profile:
  cat << EOF >> etc/profile
# Environment variables for the Qt package:
QTDIR=/usr/lib/qt
CPLUS_INCLUDE_PATH=\$QTDIR/include:\$CPLUS_INCLUDE_PATH
export QTDIR
export CPLUS_INCLUDE_PATH
EOF
fi
# OK, now we need to do the same thing to etc/csh.login:
if fgrep "QTDIR/usr/lib/qt" etc/csh.login 1> /dev/null 2> /dev/null ; then
  # Good, we've already found the Qt stuff in etc/csh.login, so we
  # won't need to add it again.
  FOUND=GOOD # :^)
else
  cat << EOF >> etc/csh.login
# Environment path variables for the Qt package:
if ( ! \$?QTDIR ) then
    setenv QTDIR /usr/lib/qt
endif
if ( \$?CPLUS_INCLUDE_PATH ) then
    setenv CPLUS_INCLUDE_PATH \$QTDIR/include:\$CPLUS_INCLUDE_PATH
else
    setenv CPLUS_INCLUDE_PATH \$QTDIR/include
endif
EOF
fi
( cd usr/bin ; rm -rf moc )
( cd usr/bin ; ln -sf /usr/lib/qt-1.42/bin/moc moc )
# Make symbolic links needed by Qt:
( cd usr/lib ; rm -rf qt )
( cd usr/lib ; ln -sf qt-1.42 qt )
( cd usr/lib/qt-1.42/include ; rm -rf qarray.h )
( cd usr/lib/qt-1.42/include ; ln -sf ../src/tools/qarray.h qarray.h )
( cd usr/lib/qt-1.42/include ; rm -rf qbitarray.h )
( cd usr/lib/qt-1.42/include ; ln -sf ../src/tools/qbitarray.h qbitarray.h )
( cd usr/lib/qt-1.42/include ; rm -rf qbuffer.h )
( cd usr/lib/qt-1.42/include ; ln -sf ../src/tools/qbuffer.h qbuffer.h )
( cd usr/lib/qt-1.42/include ; rm -rf qcache.h )
( cd usr/lib/qt-1.42/include ; ln -sf ../src/tools/qcache.h qcache.h )
( cd usr/lib/qt-1.42/include ; rm -rf qcollection.h )
( cd usr/lib/qt-1.42/include ; ln -sf ../src/tools/qcollection.h qcollection.h )
( cd usr/lib/qt-1.42/include ; rm -rf qdatastream.h )
( cd usr/lib/qt-1.42/include ; ln -sf ../src/tools/qdatastream.h qdatastream.h )
( cd usr/lib/qt-1.42/include ; rm -rf qdatetime.h )
( cd usr/lib/qt-1.42/include ; ln -sf ../src/tools/qdatetime.h qdatetime.h )
( cd usr/lib/qt-1.42/include ; rm -rf qdict.h )
( cd usr/lib/qt-1.42/include ; ln -sf ../src/tools/qdict.h qdict.h )
( cd usr/lib/qt-1.42/include ; rm -rf qdir.h )
( cd usr/lib/qt-1.42/include ; ln -sf ../src/tools/qdir.h qdir.h )
( cd usr/lib/qt-1.42/include ; rm -rf qfile.h )
( cd usr/lib/qt-1.42/include ; ln -sf ../src/tools/qfile.h qfile.h )
( cd usr/lib/qt-1.42/include ; rm -rf qfiledefs.h )
( cd usr/lib/qt-1.42/include ; ln -sf ../src/tools/qfiledefs.h qfiledefs.h )
( cd usr/lib/qt-1.42/include ; rm -rf qfileinfo.h )
( cd usr/lib/qt-1.42/include ; ln -sf ../src/tools/qfileinfo.h qfileinfo.h )
