( cd usr/bin ; rm -rf vi )
( cd usr/bin ; ln -sf elvis vi )
( cd usr/bin ; rm -rf ex )
( cd usr/bin ; ln -sf elvis ex )
( cd usr/man/man1 ; rm -rf vi.1.gz )
( cd usr/man/man1 ; ln -sf elvis.1.gz vi.1.gz )
( cd usr/man/man1 ; rm -rf ex.1.gz )
( cd usr/man/man1 ; ln -sf elvis.1.gz ex.1.gz )
( cd usr/doc ; rm -rf elvis-2.1 )
( cd usr/doc ; ln -sf /usr/share/elvis-2.1 elvis-2.1 )
